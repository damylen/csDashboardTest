{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --modern",
    "build": "vue-cli-service build --modern",
    "linkcs": "yarn link @csnext/cs-core && yarn link @csnext/cs-client && yarn link @csnext/cs-split-panel && yarn link @csnext/cs-map",
    "unlinkcs": "yarn unlink @csnext/cs-core && yarn unlink @csnext/cs-client && yarn unlink @csnext/cs-split-panel && yarn unlink @csnext/cs-map",
    "dev": "yarn serve",
    "generate": "tsc src/generators/all.ts && node src/generators/all.js",
    "build:electron": "vue-cli-service build:electron",
    "serve:electron": "vue-cli-service serve:electron"
  },
  "dependencies": {
    "@csnext/cs-client": "^0.0.75",
    "@csnext/cs-core": "^0.0.75",
    "@csnext/cs-split-panel": "^0.0.75",
    "@csnext/cs-map": "^0.0.75",
    "@csnext/cs-billboard":"^0.0.75",
    "@reactivex/rxjs": "^6.3.3",
    "class-transformer": "^0.1.10",
    "geojson": "^0.5.0",
    "axios": "~0.18.0",    
    "mapbox-gl": "^0.51.0-beta.2",
    "source-map-support": "^0.5.9",
    "vue": "^2.5.17",
    "vue-class-component": "^6.3.2",
    "vue-markdown": "^2.2.4",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.0.1",
    "vue-slider-component": "^2.8.0",
    "vuetify": "1.3.5",
    "vue-perfect-scrollbar":"^0.1.0",
    "socket.io-client":"^2.1.1"
  },
  "devDependencies": {
    "@types/socket.io-client":"^1.4.32",
    "@csnext/vue-cli-plugin-cs": "^0.0.75",
    "@types/chroma-js": "^1.4.0",
    "@vue/cli-plugin-babel": "^3.1.0",
    "@vue/cli-plugin-typescript": "^3.1.0",
    "@vue/babel-preset-app": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "dojo-typings": "^1.11.9",
    "raw-loader": "^0.5.1",
    "typescript": "^3.1.6",
    "vue-template-compiler": "^2.5.17",
    "@types/node": "^10.12.2",
    "@types/d3": "5.0.1",
    "@types/vue-markdown": "^2.2.1",
    "@types/mapbox-gl": "^0.49.1",
    "babel-runtime": "^6.26.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "build": {
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json",
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  }
}
